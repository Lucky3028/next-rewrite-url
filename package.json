{
  "name": "@lucky3028/next-rewrite-url",
  "version": "0.1.0",
  "author": "Lucky3028",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/libs/index.js",
  "types": "./dist/libs/index.d.ts",
  "bin": {
    "next-override-url": "./bin/next-override-url.mjs"
  },
  "engines": {
    "node": ">=14.16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lucky3028/next-rewrite-url.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "run-s build \"start {@}\" --",
    "start": "./bin/next-override-url.mjs",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "lint": "run-s es format",
    "lint:fix": "run-s es:fix format:fix",
    "es": "eslint 'src/**/*.ts'",
    "es:fix": "eslint --fix 'src/**/*.ts'",
    "format": "prettier --check --loglevel=warn 'src/**/*.ts'",
    "format:fix": "prettier --write --loglevel=warn 'src/**/*.ts'",
    "test": "vitest run",
    "postinstall": "typesync",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "bin/*.mjs": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/eslint": "^8.21.1",
    "@types/node": "^18.15.1",
    "@types/prettier": "^2.7.2",
    "@types/yargs": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "happy-dom": "^8.9.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.3",
    "tslib": "^2.5.0",
    "typescript": "^5.0.0",
    "typesync": "^0.10.0",
    "vite": "^4.2.0",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.29.3"
  },
  "dependencies": {
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "yargs": "^17.7.1",
    "zod": "^3.21.4"
  },
  "volta": {
    "node": "18.15.0",
    "yarn": "1.22.19"
  }
}
