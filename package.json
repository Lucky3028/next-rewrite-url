{
  "name": "next-rewrite-url",
  "version": "0.1.0",
  "author": "Lucky3028",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "start": "ts-node --esm ./src/index.ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "lint": "run-s es format",
    "lint:fix": "run-s es:fix format:fix",
    "es": "eslint 'src/**/*.ts'",
    "es:fix": "eslint --fix 'src/**/*.ts'",
    "format": "prettier --check --loglevel=warn 'src/**/*.ts'",
    "format:fix": "prettier --write --loglevel=warn 'src/**/*.ts'",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --passWithNoTests",
    "test:coverage": "yarn test --ci --passWithNoTests",
    "postinstall": "typesync",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/eslint": "^8.21.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.1",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.3",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "typesync": "^0.10.0"
  },
  "volta": {
    "node": "18.15.0",
    "yarn": "1.22.19"
  }
}
