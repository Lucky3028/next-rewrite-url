{
  "name": "@lucky3028/next-rewrite-url",
  "version": "0.8.1",
  "author": "Lucky3028",
  "license": "MIT",
  "type": "module",
  "exports": {
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.cjs",
    "node": "./dist/cjs/index.cjs",
    "default": "./dist/cjs/index.cjs"
  },
  "bin": {
    "next-rewrite-url": "./bin/next-rewrite-url.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lucky3028/next-rewrite-url.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "run-s build \"start {@}\" --",
    "start": "./bin/next-rewrite-url.mjs",
    "build": "rollup -c",
    "lint": "run-s es format",
    "lint:fix": "run-s es:fix format:fix",
    "es": "eslint 'src/**/*.ts'",
    "es:fix": "eslint --fix 'src/**/*.ts'",
    "format": "prettier --check --loglevel=warn 'src/**/*.ts'",
    "format:fix": "prettier --write --loglevel=warn 'src/**/*.ts'",
    "test": "vitest run",
    "postinstall": "typesync || :",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "bin/next-rewrite-url.mjs": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "yargs": "^17.7.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/eslint": "^8.37.0",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/lint-staged": "^13.2.0",
    "@types/lodash": "^4.14.194",
    "@types/prettier": "^2.7.2",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "happy-dom": "^9.6.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "rollup": "^3.20.2",
    "typescript": "^5.0.4",
    "typesync": "^0.11.0",
    "vite": "^4.2.1",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.0"
  },
  "volta": {
    "node": "18.16.0",
    "yarn": "1.22.19"
  }
}
